    "components": {
        "schemas": {
            "ErrorResponse": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "code": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "category": {
                        "type": "string",
                        "enum": [
                            "INFO",
                            "DEBUG",
                            "WARN",
                            "FATAL",
                            "ERROR"
                        ]
                    },
                    "description": {
                        "type": "string"
                    },
                    "externalDescription": {
                        "type": "string"
                    },
                    "legacyCode": {
                        "type": "string"
                    },
                    "errors": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            },
            "CoverageRequest": {
                "required": [
                    "id",
                    "nombre"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "pattern": "^(1|2|3|5|9)$",
                        "type": "string",
                        "description": "Id de la cobertura.",
                        "example": "1"
                    },
                    "nombre": {
                        "type": "string",
                        "description": "Nombre de la cobertura.",
                        "example": "Muerte accidental"
                    },
                    "sumaAsegurada": {
                        "type": "integer",
                        "description": "Suma asegurada de la cobertura adicional.",
                        "format": "int32",
                        "example": 100000
                    },
                    "prima": {
                        "type": "number",
                        "description": "Prima de la cobertura adicional.",
                        "format": "double",
                        "example": 34.78
                    }
                },
                "description": "Lista de coberturas adicionales.",
                "example": []
            },
            "SendAplicationPolicyVdtRequest": {
                "required": [
                    "beneficiarios",
                    "codigoProducto",
                    "correo",
                    "fechaNacimiento",
                    "fechaVenta",
                    "modalidadPago",
                    "montoRetiro",
                    "nombreCompleto",
                    "numeroTarjeta",
                    "porcentajeTotal",
                    "primaComercial",
                    "primerNombre",
                    "sumaAseguradaFallecimiento"
                ],
                "type": "object",
                "properties": {
                    "tipoDocumento": {
                        "pattern": "^(DNI|CE|PASAPORTE)$",
                        "type": "string",
                        "description": "Tipo de documento del contratante.",
                        "example": "DNI"
                    },
                    "primerNombre": {
                        "type": "string",
                        "description": "Primer nombre del contratante.",
                        "example": "Pedro"
                    },
                    "nombreCompleto": {
                        "type": "string",
                        "description": "Nombre completo del contratante.",
                        "example": "Pedro Sanchez Baca"
                    },
                    "numeroDocumento": {
                        "pattern": "^\\d{8,15}$",
                        "type": "string",
                        "description": "Numero de Documento del contratante.",
                        "example": "74201182"
                    },
                    "numeroSolicitud": {
                        "pattern": "^\\d{4,9}$",
                        "type": "string",
                        "description": "Numero de Solicitud de Poliza.",
                        "example": "5645534"
                    },
                    "fechaNacimiento": {
                        "type": "string",
                        "description": "Fecha de Nacimiento del contratante.",
                        "format": "date",
                        "example": "1998-10-12"
                    },
                    "sexo": {
                        "pattern": "^(Hombre|Mujer)$",
                        "type": "string",
                        "description": "Sexo biologico del contratante.",
                        "example": "Hombre"
                    },
                    "telefono": {
                        "pattern": "^(9\\d{8})$",
                        "type": "string",
                        "description": "Telefono del contratante.",
                        "example": "942396369"
                    },
                    "direccion": {
                        "type": "string",
                        "description": "Direccion del contratante.",
                        "example": "Av las camelias 165 - San Isidro"
                    },
                    "peso": {
                        "pattern": "^([1-9][0-9]\\.[0-9])$",
                        "type": "string",
                        "description": "Peso del contratante.",
                        "example": "80.5"
                    },
                    "talla": {
                        "pattern": "^(1\\.[0-9]{2})$",
                        "type": "string",
                        "description": "Talla del contratante.",
                        "example": "1.75"
                    },
                    "obligacionFiscal": {
                        "pattern": "^(SI|NO)$",
                        "type": "string",
                        "description": "Obligacion fiscal en pais extranjero.",
                        "example": "SI"
                    },
                    "pais": {
                        "type": "string",
                        "description": "Pais de nacimiento.",
                        "example": "Perú"
                    },
                    "coberturas": {
                        "type": "array",
                        "description": "Lista de coberturas adicionales.",
                        "example": [],
                        "items": {
                            "$ref": "#/components/schemas/CoverageRequest"
                        }
                    },
                    "correo": {
                        "type": "string",
                        "description": "Email del cliente",
                        "example": "josuecd98@outlook.com"
                    },
                    "sumaAseguradaFallecimiento": {
                        "maximum": 500000,
                        "minimum": 100000,
                        "type": "integer",
                        "description": "Suma asegurada en caso de fallecimiento.",
                        "format": "int32",
                        "example": 100000
                    },
                    "beneficioMuerteNatural": {
                        "type": "integer",
                        "description": "Beneficio de Muerte Natural.",
                        "format": "int32",
                        "example": 100000
                    },
                    "beneficioMuerteAccidental": {
                        "type": "integer",
                        "description": "Beneficio de Muerte Accidental.",
                        "format": "int32",
                        "example": 100000
                    },
                    "beneficioInvalidezAccidental": {
                        "type": "integer",
                        "description": "Beneficio de Invalidez Accidental.",
                        "format": "int32",
                        "example": 100000
                    },
                    "beneficioEnfermedad": {
                        "type": "integer",
                        "description": "Benefico por enfermedad.",
                        "format": "int32",
                        "example": 100000
                    },
                    "beneficioExoneracionPrimas": {
                        "type": "integer",
                        "description": "Benefico por exoneracion de primas.",
                        "format": "int32",
                        "example": 100000
                    },
                    "plazoContratacion": {
                        "pattern": "^(5|7|10|12|15|20)$",
                        "type": "string",
                        "description": "Plazo de Contratacion.",
                        "example": "7"
                    },
                    "primaComercial": {
                        "type": "number",
                        "description": "Prima Comercial.",
                        "format": "double",
                        "example": 10.57
                    },
                    "montoRetiro": {
                        "type": "number",
                        "description": "Monto de retiro al final del periodo.",
                        "format": "double",
                        "example": 10400
                    },
                    "beneficiario1": {
                        "pattern": "^([\\w\\s]{0,20}[\\|]{1}){4}(\\d){1,2}$",
                        "type": "string",
                        "description": "Primer Beneficiario.",
                        "example": "Marquez|Jimenez|Arian Daniel|Hijo|50"
                    },
                    "beneficiario2": {
                        "pattern": "^([\\w\\s]{0,20}[\\|]{1}){4}(\\d){1,2}$",
                        "type": "string",
                        "description": "Segundo Beneficiario.",
                        "example": "Marquez|Jimenez|Arian Daniel|Hijo|50"
                    },
                    "beneficiario3": {
                        "pattern": "^([\\w\\s]{0,20}[\\|]{1}){4}(\\d){1,2}$",
                        "type": "string",
                        "description": "Tercer Beneficiario.",
                        "example": "Marquez|Jimenez|Arian Daniel|Hijo|50"
                    },
                    "beneficiario4": {
                        "pattern": "^([\\w\\s]{0,20}[\\|]{1}){4}(\\d){1,2}$",
                        "type": "string",
                        "description": "Cuarto Beneficiario.",
                        "example": "Marquez|Jimenez|Arian Daniel|Hijo|50"
                    },
                    "beneficiario5": {
                        "pattern": "^([\\w\\s]{0,20}[\\|]{1}){4}(\\d){1,2}$",
                        "type": "string",
                        "description": "Quinto Beneficiario.",
                        "example": "Marquez|Jimenez|Arian Daniel|Hijo|50"
                    },
                    "beneficiario6": {
                        "pattern": "^([\\w\\s]{0,20}[\\|]{1}){4}(\\d){1,2}$",
                        "type": "string",
                        "description": "Sexto Beneficiario.",
                        "example": "Marquez|Jimenez|Arian Daniel|Hijo|50"
                    },
                    "porcentajeTotal": {
                        "pattern": "^(50|100|120|150|200)$",
                        "type": "string",
                        "description": "Porcentaje Total.",
                        "example": "100"
                    },
                    "fechaVenta": {
                        "type": "string",
                        "description": "Fecha de Venta.",
                        "format": "date",
                        "example": "0018-06-15"
                    },
                    "modalidadPago": {
                        "pattern": "^(DEBITO|CREDITO)$",
                        "type": "string",
                        "description": "Modalidad de Pago.",
                        "example": "DEBITO"
                    },
                    "beneficiarios": {
                        "pattern": "^(herederos|otros)$",
                        "type": "string",
                        "description": "Beneficiarios del Seguro.",
                        "example": "herederos"
                    },
                    "numeroTarjeta": {
                        "pattern": "^\\d{16}$",
                        "type": "string",
                        "description": "Numero de Tarjeta.",
                        "example": "4111222233334444"
                    },
                    "usoAdicional": {
                        "pattern": "^(SI|NO)$",
                        "type": "string",
                        "description": "Usos Adicionales.",
                        "example": "SI"
                    },
                    "frecuenciaPago": {
                        "pattern": "^(1|6|12)$",
                        "type": "string",
                        "description": "Frecuencia de Pago.",
                        "example": "12"
                    },
                    "codigoProducto": {
                        "type": "string",
                        "description": "Codigo del producto.",
                        "example": "831"
                    }
                }
            },
            "NotificationRequest": {
                "type": "object",
                "properties": {
                    "datos": {
                        "type": "string",
                        "description": "contiene los datos del Request de cada operacion",
                        "example": "43"
