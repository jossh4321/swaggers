                        "type": "string",
                        "description": "Nombres.",
                        "example": "JOSE MARIANO"
                    },
                    "apellidoPaterno": {
                        "type": "string",
                        "description": "Apellido Paterno.",
                        "example": "PEREZ"
                    },
                    "apellidoMaterno": {
                        "type": "string",
                        "description": "Apellido Materno.",
                        "example": "SANCHEZ"
                    },
                    "numeroDocumento": {
                        "type": "string",
                        "description": "Numero de Documento.",
                        "example": "75201182"
